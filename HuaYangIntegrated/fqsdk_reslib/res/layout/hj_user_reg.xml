                            android:layout_height="wrap_content"
                            android:layout_alignBottom="@+id/hj_regtypeapp"
                            android:layout_alignTop="@+id/hj_regtypeapp"
                            android:layout_centerHorizontal="true"
                            android:background="@color/hj_newhintdeepgrey" />

                        <LinearLayout
                            android:id="@+id/hj_regtypephone"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_toRightOf="@+id/hj_regtypeapp"
                            android:gravity="center"
                            android:orientation="horizontal" >

                            <ImageView
                                android:id="@+id/hj_imgphoneicon"
                                android:layout_width="@dimen/hj_smallicon_size"
                                android:layout_height="@dimen/hj_smallicon_size"
                                android:src="@drawable/fq_phone_gray" />

                            <TextView
                                android:id="@+id/hj_textphonetype"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="3dp"
                                android:layout_marginRight="2dp"
