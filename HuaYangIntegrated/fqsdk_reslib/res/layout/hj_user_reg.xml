                        android:gravity="center_vertical"
                        android:orientation="horizontal"
                        android:padding="5dp" >

                        <LinearLayout
                            android:id="@+id/hj_regtypeapp"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true"
                            android:layout_alignRight="@+id/hj_cutoffrule"
                            android:gravity="center"
                            android:orientation="horizontal" >

                            <ImageView
                                android:id="@+id/hj_imgacticon"
                                android:layout_width="@dimen/hj_smallicon_size"
                                android:layout_height="@dimen/hj_smallicon_size"
                                android:src="@drawable/fq_accounticon" />

                            <TextView
                                android:id="@+id/hj_textacttype"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="3dp"
                                android:layout_marginRight="2dp"
                                android:gravity="center"
                                android:text="@string/hj_btn_regtypeapp"
                                android:textColor="@color/hj_new_orange"
                                android:textSize="@dimen/hj_font_size" />

                            <ImageView
                                android:id="@+id/hj_imgactleft"
