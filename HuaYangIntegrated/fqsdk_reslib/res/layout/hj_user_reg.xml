                                android:id="@+id/hj_imgphoneleft"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginRight="0dp"
                                android:padding="2dp"
                                android:src="@drawable/fq_rightarrow_gray" />
                        </LinearLayout>
                    </RelativeLayout>
<RelativeLayout android:id="@+id/fq_center_frame"
