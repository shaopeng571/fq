                                    android:imeOptions="flagNoExtractUi"
                                    android:maxLength="20"
                                    android:padding="4dp"
                                    android:singleLine="true"
                                    android:textColor="@color/hj_base_black"
                                    android:textColorHint="@color/hj_newhintgrey"
                                    android:textSize="@dimen/hj_font_size" >
                                </EditText>
                            </LinearLayout>
                        </RelativeLayout>

                        <LinearLayout
                            android:id="@+id/hj_reggetcodelin_phone"
                            android:layout_width="80dp"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_alignParentTop="true"
                            android:layout_marginTop="5dp"
                            android:orientation="vertical"
                            android:visibility="visible" >

                            <Button
                                android:id="@+id/hj_btn_getcode_phone"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/fq_login_register_frame_height"
                                android:background="@drawable/fq_getcodeshape_orange"
                                android:gravity="center"
                                android:text="@string/hj_btn_reggetcode"
                                android:textColor="@color/hj_base_white"
                                android:textSize="@dimen/hj_font_size" />

                         
                        </LinearLayout>
                    </RelativeLayout>
</RelativeLayout>
                    

                    <LinearLayout
                        android:id="@+id/hj_readlin"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/fq_center_frame"
                        android:layout_marginBottom="12dp"
                        android:layout_marginTop="12dp"
                        android:gravity="center_vertical"
                        android:orientation="horizontal" >

                        <CheckBox
                            android:id="@+id/cb_user_agreement"
                            style="@style/HJCustomCheckboxTheme"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="4dp"
                            android:layout_marginRight="1dp"
                            android:checked="true" />

                        <TextView
                            android:id="@+id/txt_1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="1dp"
                            android:text="@string/hj_txt_regiread"
